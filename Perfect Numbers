https://www.geeksforgeeks.org/problems/perfect-numbers3207/1
class Solution {
    static boolean isPerfect(int n) {
        if(n == 1) return false;
        int sum = 1;
        for (int i = 2; i * i <= n; i++) {
    if (n % i == 0) {
        sum += i;
        int divPair = n / i;
        if (divPair != i) {
            sum += divPair;
        }
    }
}

        return n == sum;
    }
};
