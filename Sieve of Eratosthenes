https://www.geeksforgeeks.org/problems/sieve-of-eratosthenes5242/1
class Solution:
    def sieve(self, n):
        prime_list =[True]*(n+1)
        prime_list[0] = False
        prime_list[1] = False
        p = 2
        while p*p <= n:
            if prime_list[p]:
                for i in range(p*p,n+1,p):
                    prime_list[i] = False
                    
            p = p+1
            
        primes = [i for i in range(n+1) if prime_list[i]]
        return primes
